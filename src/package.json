{
  "name": "hupper",
  "version": "2.2.0",
  "description": "Improve the user experience of HUP.hu",
  "main": "",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "$(npm bin)/tape -r babel-register test/{lib,core,data}/**/*.js",
    "testData": "$(npm bin)/tape -r babel-register test/data/**/*.js",
    "coverage": "$(npm bin)/nyc --reporter=lcov --reporter=text $(npm bin)/tape -r babel-register test/{core,lib}/*.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Ajnasz/hupper.git"
  },
  "author": "Lajos Koszti <ajnasz@ajnasz.hu>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Ajnasz/hupper/issues"
  },
  "homepage": "https://github.com/Ajnasz/hupper",
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-preset-es2015": "^6.14.0",
    "babelify": "^7.3.0",
    "browserify": "^13.1.1",
    "eslint": "^3.12.2",
    "nyc": "^10.1.2"
  },
  "dependencies": {
    "babel-plugin-istanbul": "^3.1.2",
    "babel-plugin-transform-es2015-modules-umd": "^6.12.0",
    "jsdom": "^9.11.0",
    "tape": "^4.6.3"
  },
  "nyc": {
    "require": [
      "babel-register"
    ],
    "sourceMap": true,
    "instrument": true
  }
}
