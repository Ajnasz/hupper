{
	"env": {
		"es6": true,
		"browser": true,
		"node": true
	},

	"parserOptions": {
		"sourceType": "module"
	},

	"rules": {
		"no-floating-decimal": 0,
		"no-bitwise": 0,
		"no-new-func": 0,
		"no-underscore-dangle": 0,
		"smarter-eqeqeq": 0,
		"guard-for-in": 0,
		"brace-style": 0,
		"eol-last": 0,
		"no-shadow": 0,

		"dot-notation": 1,
		"indent": [1, "tab"],
		"max-len": [1, 160],
		"no-console": 0,
		"no-debugger": 1,
		"no-fallthrough": 1,
		"no-unused-vars": 1,
		"one-var": [0],
		"radix": 1,

		"camelcase": 2,
		"comma-dangle": 0,
		"complexity": [2, 13],
		"curly": 2,
		"eqeqeq": 2,
		"keyword-spacing": 2,
		"new-cap": 2,
		"new-parens": 2,
		"no-alert": 2,
		"no-caller": 2,
		"no-empty": 2,
		"no-eq-null": 2,
		"no-eval": 2,
		"no-extra-semi": 2,
		"no-implied-eval": 2,
		"no-loop-func": 2,
		"no-new-wrappers": 2,
		"no-octal": 2,
		"no-shadow-restricted-names": 2,
		"no-undef": 2,
		"no-undef-init": 2,
		"no-unreachable": 2,
		"no-with": 2,
		"quotes": [2, "single", "avoid-escape"],
		"semi": 2,
		"space-before-function-paren": 2,
		"use-isnan": 2,
		"no-trailing-spaces": "error"
	}
}
