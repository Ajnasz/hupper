{
	"env": {
		"es6": true,
		"browser": true
	},
	"rules": {
		"no-floating-decimal": 0,
		"no-bitwise": 0,
		"no-new-func": 0,
		"no-underscore-dangle": 0,
		"smarter-eqeqeq": 0,
		"guard-for-in": 0,
		"brace-style": 0,
		"eol-last": 0,
		"no-shadow": 0,
		"no-unused-vars": 0,
		"no-shadow-restricted-names": 2,

		"radix": 1,
		"dot-notation": 1,
		"no-fallthrough": 1,

		"no-caller": 2,
		"no-console": 0,
		"no-debugger": 2,
		"no-empty": 2,
		"no-eval": 2,
		"camelcase": 2,
		"curly": 2,
		"new-cap": 2,
		"semi": [2, "always"],
		"use-isnan": 2,
		"quotes": [2, "single"],
		"max-len": [2, 140, 4],
		"new-parens": 2,
		"eqeqeq": 2,
		"no-new": 2,
		"no-new-wrappers": 2,
		"no-octal": 2,
		"no-unreachable": 2,
		"no-undef-init": 2,
		"no-implied-eval": 2,
		"no-with": 2
	}
}
